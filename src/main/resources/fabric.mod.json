{
  "schemaVersion": 1,
  "id": "playertrack",
  "version": "${version}",
  "name": "PlayerTrack",
  "description": "A mod notifying you of the presence of a player specified in the configs next to you.",
  "authors": [
    "faiks12"
  ],
  "contact": {
    "sources": "https://github.com/faiks11/playertrack"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/korobka/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "md.faiks.playertrack.PlayerTrack"
    ],
    "modmenu": [
      "md.faiks.playertrack.CONFIG.modMenu"
    ]
  },
  "mixins": [
    "korobka.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": "*"
  },
  "suggests": {
    "modmenu": ">=9"
  },
  "custom": {
    "modmanager": {
      "modrinth": "cOJARDRW"
    }
  }
}